<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ch.nike.mapper.StoreMapper">
	<select id="storelist" resultType="store">
		select	*
		from	store
	order by	store_no desc
	</select>
</mapper>